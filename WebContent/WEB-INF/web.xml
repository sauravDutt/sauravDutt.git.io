    <servlet-name>s1</servlet-name>
    <servlet-class>pack1.MyServlet</servlet-class>
    <init-param>
      <param-name>ps1</param-name>
      <param-value>advance java one</param-value>
    </init-param>
    <init-param>
      <param-name>ps2</param-name>
      <param-value>advance java two</param-value>
    </init-param>
    <init-param>
      <param-name>ps3</param-name>
      <param-value>advance java three</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>s1</servlet-name>
    <url-pattern>/abcd</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>s2</servlet-name>
    <servlet-class>pack1.MyGenericServlet</servlet-class>
    <init-param>
      <param-name>p1</param-name>
      <param-value>jdbc.mysql.mysqlDialect</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>s2</servlet-name>
    <url-pattern>/genric</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>c1</param-name>
    <param-value>context param working...</param-value>
  </context-param>
